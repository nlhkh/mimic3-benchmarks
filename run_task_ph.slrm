#!/bin/bash
#SBATCH -t 2-04:00:00
#SBATCH -n 1
#SBATCH --gres=gpu:1
#SBATCH -C 'pascal|kepler|volta'
#SBATCH --mem=10G
#SBATCH --mail-type=ALL
#SBATCH --mail-user=kha.nguyen@aalto.fi
#SBATCH -p gpu

model=$1
dropout=$2
mode=$3

module load anaconda3
source activate mimic3

srun python -um mimic3models.phenotyping.main --network mimic3models/keras_models/$model.py --dim 256 --timestep 1.0 --depth 1 --dropout $dropout --mode $mode --batch_size 8 --output_dir mimic3models/phenotyping

